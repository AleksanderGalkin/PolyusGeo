@{
    Layout = null;
}

    <!-- Заголовок модального окна -->
        <div class="modal-header">
            <button type="button" class="close"  aria-hidden="true"  ng-click="cancel()">×</button>
            <h5 class="modal-title">Заголовок</h5>
        </div>
        <!-- Основное содержимое модального окна -->
        <div class="modal-body">
            <div class="container-fluid">
                <div><label for="S_SAMP">Верхняя проба: </label><input id="S_SAMP" ng-model="dtm.startSample"/></div>
                <div><label for="COL_SAMP">Количество проб: </label><input id="COL_SAMP" ng-model="dtm.numSample" /></div>

            </div>
        </div>

    
    

    

        <!-- Футер модального окна -->
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" ng-click="ok()">Начать</button>
            <button type="button" class="btn btn-warning" ng-click="cancel()">Закрыть</button>
        </div> 
